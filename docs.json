{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "maple",
  "name": "Tibbie",
  "description": "Documentation for Tibbie, a social media scheduling and automation platform. Connect accounts, compose posts, and publish on a schedule.",
  "colors": {
    "primary": "#FE5F38",
    "light": "#FF9278",
    "dark": "#E84E2A"
  },
  "favicon": "/favicon.svg",
  "fonts": {
    "heading": {
      "family": "GT America",
      "source": "/fonts/gt-america-medium.woff2",
      "format": "woff2",
      "weight": 600
    },
    "body": {
      "family": "GT America",
      "source": "/fonts/gt-america-regular.woff2",
      "format": "woff2"
    }
  },
  "logo": {
    "light": "/logo/light.svg",
    "dark": "/logo/dark.svg",
    "height": 24
  },
  "navigation": {
    "tabs": [
      {
        "tab": "Guides",
        "groups": [
          {
            "group": "Getting started",
            "pages": [
              "index",
              "quickstart"
            ]
          },
          {
            "group": "Features",
            "pages": [
              "features/scheduling",
              "features/multi-platform",
              "features/media",
              "features/calendar",
              "features/platform-limits"
            ]
          },
          {
            "group": "Connections",
            "pages": [
              "connections/connecting",
              "connections/reconnecting",
              "connections/disconnecting"
            ]
          },
          {
            "group": "CLI",
            "pages": [
              "cli/getting-started"
            ]
          },
          {
            "group": "MCP Server",
            "pages": [
              "mcp/overview"
            ]
          },
          {
            "group": "Billing",
            "pages": [
              "billing/plans",
              "billing/usage"
            ]
          }
        ]
      },
      {
        "tab": "API Reference",
        "icon": "code",
        "openapi": "api-reference/openapi.json",
        "groups": [
          {
            "group": "Getting started",
            "pages": [
              "api-reference/authentication",
              "api-reference/using-the-api",
              "api-reference/rate-limiting"
            ]
          },
          {
            "group": "User",
            "pages": [
              "GET /user"
            ]
          },
          {
            "group": "Connections",
            "pages": [
              "POST /connections/connect/{platform}",
              "GET /connections/connect/{platform}/status/{stateToken}",
              "GET /connections",
              "GET /connections/{connection}",
              "DELETE /connections/{connection}"
            ]
          },
          {
            "group": "Posts",
            "pages": [
              "GET /posts",
              "POST /posts",
              "GET /posts/{post}",
              "PATCH /posts/{post}",
              "DELETE /posts/{post}",
              "POST /posts/{post}/publish"
            ]
          },
          {
            "group": "Media",
            "pages": [
              "POST /media"
            ]
          }
        ]
      }
    ],
    "global": {
      "anchors": [
        {
          "anchor": "Documentation",
          "href": "https://tibbie.co/docs",
          "icon": "book-open-cover"
        },
        {
          "anchor": "Blog",
          "href": "https://tibbie.co/blog",
          "icon": "newspaper"
        }
      ]
    }
  },
  "navbar": {
    "links": [
      {
        "label": "Support",
        "href": "mailto:hi@tibbie.co"
      }
    ],
    "primary": {
      "type": "button",
      "label": "Dashboard",
      "href": "https://tibbie.co/posts"
    }
  },
  "api": {
    "playground": {
      "display": "interactive"
    },
    "examples": {
      "languages": ["bash", "python", "javascript", "node"]
    }
  },
  "search": {
    "prompt": "Search Tibbie docs..."
  },
  "seo": {
    "indexing": "navigable"
  },
  "metadata": {
    "timestamp": true
  },
  "contextual": {
    "options": [
      "copy",
      "view",
      "chatgpt",
      "claude",
      "perplexity",
      "mcp",
      "cursor",
      "vscode"
    ]
  },
  "footer": {
    "socials": {
      "x": "https://x.com/danielpost"
    }
  },
  "errors": {
    "404": {
      "redirect": false,
      "title": "Page not found",
      "description": "The page you're looking for doesn't exist. [Back to docs](/)"
    }
  }
}
